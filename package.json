  },
  "bugs": "https://github.com/vorobiovkirill/Angular-Starter-Kit/issues",
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "angular5",
    "Angular Material",
    "typescript",
    "sass",
    "scss",
    "bourbon-neat",
    "hot",
    "reload",
    "live",
    "webpack",
    "boilerplate",
    "starter",
    "Autoprefixer",
    "normalize.css"
  ],
  "scripts": {
    "build:dev": "npm run clean:dist && ENV=dev webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "npm run clean:dist && ENV=prod webpack --config config/webpack.prod.js --progress --profile --bail",
    "build": "npm run build:dev",
    "clean:dist": "rimraf dist coverage documentation",
    "clean:install": "npm cache clean --force && rimraf node_modules dist coverage && npm install",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "sass-lint --config ./.sass-lint.yml --no-exit --verbose",
    "lint:js": "tslint \"src/**/*.ts\" --config ./tslint.json --project ./tsconfig.json --type-check --format codeFrame",
    "start": "npm run start:server",
    "start:server": "webpack-dev-server --config config/webpack.dev.js --progress --colors",
    "watch": "npm run start",
    "rimraf": "rimraf"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
